{
  "routes": [
    {
      "route": "/api",
      "rewrite": "https://t6gymccrfg.execute-api.us-east-1.amazonaws.com/prod/api"
    },
    {
      "route": "/api/*",
      "rewrite": "https://t6gymccrfg.execute-api.us-east-1.amazonaws.com/prod/api/{*path}"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/*.ico",
      "/*.json",
      "/api/*"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".svg": "image/svg+xml"
  }
}
