{
  "routes": [
    {
      "route": "/api",
      "allowedRoles": ["anonymous"],
      "rewrite": "https://t6gymccrfg.execute-api.us-east-1.amazonaws.com/prod"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "rewrite": "https://t6gymccrfg.execute-api.us-east-1.amazonaws.com/prod/{*path}"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/*.ico",
      "/*.json",
      "/assets/*"
    ]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".svg": "image/svg+xml"
  }
}
