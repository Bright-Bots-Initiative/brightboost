{
  "name": "brightboost-backend",
  "private": true,
  "version": "0.1.0",
  "main": "dist/src/server.js",
  "scripts": {
    "db:generate": "prisma generate --schema=../prisma/schema.prisma && CLIENT_PATH=$(readlink -f ../node_modules/@prisma/client) && rm -rf node_modules/.prisma node_modules/@prisma/client && cp -r \"$CLIENT_PATH/../../.prisma\" node_modules/ && mkdir -p node_modules/@prisma && cp -r \"$CLIENT_PATH\" node_modules/@prisma/",
    "db:migrate": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "build": "tsc -p .",
    "start": "node dist/src/server.js",
    "start:dev": "ts-node src/server.ts",
    "dev": "ts-node src/server.ts"
  },
  "dependencies": {
    "@prisma/client": "6.8.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.9",
    "prisma": "^6.8.2",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.15"
  }
}
